{
  "productName": "smmatool",
  "version": "0.0.0",
  "identifier": "com.smma.tool",
  "build": {
    "beforeDevCommand": "yarn run dev",
    "devUrl": "http://localhost:3000",
    "beforeBuildCommand": "yarn run generate",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "smma-tool",
        "width": 1280,
        "height": 720,
        "minWidth": 1280,
        "minHeight": 720
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "windows": {
      "allowDowngrades": true,
      "certificateThumbprint": null,
      "digestAlgorithm": null,
      "nsis": null,
      "timestampUrl": null,
      "tsp": false,
      "webviewFixedRuntimePath": null,
      "webviewInstallMode": {
        "silent": true,
        "type": "downloadBootstrapper"
      },
      "wix": null
    },
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/icon.ico"
    ]
  }
}