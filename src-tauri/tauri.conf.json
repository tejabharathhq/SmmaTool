{
  "productName": "smmatool",
  "version": "0.0.0",
  "identifier": "com.smma.tool",
  "build": {
    "beforeDevCommand": "yarn run dev",
    "devUrl": "http://localhost:3000",
    "beforeBuildCommand": "yarn run generate",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "smma-tool",
        "width": 1280,
        "height": 720,
        "minWidth": 1280,
        "minHeight": 720
      }
    ],
    "security": {
      "csp": null
    }
  },
  "plugins": {
    "updater": {
      "endpoints": [
        "http://localhost:3007"
      ],
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IEQzRkYxNzJDQUY3MDAzQzQKUldURUEzQ3ZMQmYvMDQwV1ltMk9HODFXU0ZiUjZvVGhVTjBOTlFneDhBL2lZY0RTSEZCRHdOR24K",
      "windows": {
        "installMode": "basicUi",
        "installerArgs": [
          "/NS"
        ]
      }
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}